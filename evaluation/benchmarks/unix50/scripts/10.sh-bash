#!/bin/bash

# 4.4: histogram of Belle's captures (-pawns) by each type of piece
python3 aws/s3-get-object.py "$IN" /dev/stdout | tr ' ' '\n' | grep 'x' | grep '\.' | cut -d '.' -f 2 | grep '[KQRBN]' | cut -c 1-1 | sort | uniq -c | sort -nr | python3 aws/s3-put-object.py ${OUT}stdout.txt /dev/stdin
