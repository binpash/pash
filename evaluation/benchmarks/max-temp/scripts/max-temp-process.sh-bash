#!/bin/bash

## Processing
python3 aws/s3-get-object.py "$IN" /dev/stdout |
  cut -c 88-92 |
  grep -v 999 |
  sort -rn |
  head -n 1

python3 aws/s3-get-object.py "$IN" /dev/stdout |
  cut -c 88-92 |
  grep -v 999 |
  sort -n |
  head -n 1 

python3 aws/s3-get-object.py "$IN" /dev/stdout |
  cut -c 88-92 |
  grep -v 999 |
  awk "{ total += \$1; count++ } END { print total/count }" |
  python3 aws/s3-put-object.py ${OUT}avg.txt /dev/stdin