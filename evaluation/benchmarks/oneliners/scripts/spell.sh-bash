#!/bin/bash
python3 aws/s3-get-object.py "$IN" /dev/stdout |
    # iconv -f utf-8 -t ascii//translit | # remove non utf8 characters
    col -bx |                           # remove backspaces / linefeeds
    tr -cs A-Za-z '\n' |
    tr A-Z a-z |                        # map upper to lower case
    tr -d '[:punct:]' |                 # remove punctuation
    sort |                              # put words in alphabetical orderx
    uniq |                              # remove duplicate words
    comm -23 - <(python3 aws/s3-get-object.py "$DICT" /dev/stdout) |               # report words not in dictionary 
    python3 aws/s3-put-object.py ${OUT}stdout.txt /dev/stdin
